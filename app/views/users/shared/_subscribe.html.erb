<div class="subscribe hover">
  <div class="container">
    <div class="top">
      <div class="row">
        <h4>Subscribe</h4></div>
      <div class="row">
        <a href="<%= user_facebook_omniauth_authorize_path%>" class="col-md-4">
          <i class="fa fa-facebook-square hvr-grow button"></i>
        </a>
        <a href="<%= user_twitter_omniauth_authorize_path%>" class="col-md-4">
          <i class="fa fa-twitter hvr-grow button"></i>
        </a>
        <a href="<%= user_google_oauth2_omniauth_authorize_path%>" class="col-md-4">
          <i class="fa fa-google hvr-grow button"></i>
        </a>
      </div>
    </div>
    <div class="bottom">
      <div class="row divider">
        <h5>Or Be Classical</h5></div>
      <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <div class="row name">
        <i class="fa fa-user"></i>
        <%= f.text_field :name , :class => 'subinput', :placeholder => 'Name'%>
        </label>
      </div>
      <div class="row email">
        <i class="fa fa-envelope"></i>
        <%= f.text_field :email , :class => 'subinput', :placeholder => 'Email' %>
      </div>
      <div class="row start">
        <%= f.submit 'Get Started', :class => 'link' %>
      </div>
      <% end %>
    </div>
  </div>
</div>
