<div class="subscribe hover">
  <div class="container">
    <div class="top">
      <div class="row">
        <h4>Subscribe</h4></div>
      <div class="row">
        <a href="/users/auth/facebook" class="col-md-4">
          <i class="fa fa-facebook-square hvr-grow button"></i>
        </a>
        <a href="/users/auth/twitter" class="col-md-4" style="">
          <i class="fa fa-twitter hvr-grow button"></i>
        </a>
        <a href="/users/auth/google_oauth2" class="col-md-4">
          <i class="fa fa-google hvr-grow button"></i>
        </a>
      </div>
    </div>
    <div class="bottom">
      <div class="row divider">
        <h5>Or Be Classical</h5></div>
      <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
      <div class="row name">
        <i class="fa fa-user"></i>
        <%= text_field "user", "name", "id" => "input_name", "class" => "subinput" %>
        </label>
      </div>
      <div class="row email">
        <i class="fa fa-envelope"></i>
        <%= text_field "user", "email", "id" => "input_email", "class" => "subinput" %>
      </div>
      <div class="row start">
        <%= button_tag "Get Started", "name" => "commit", "class" => "link" %>
      </div>
      <% end %>
    </div>
  </div>
</div>
